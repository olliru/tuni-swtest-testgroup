{
  "name": "tuni-swtest-testgroup",
  "version": "1.0.0",
  "description": "SW test group work",
  "main": "index.js",
  "scripts": {
    "build": "babel src/*.js -d src",
    "test": "cross-env NODE_ENV=test nyc _mocha test/*.js",
    "cover": "cross-env NODE_ENV=test nyc _mocha test/*.js",
    "mochatest": "nyc _mocha --timeout=3000",
    "coverib": "istanbul cover _mocha",
    "coveralls": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info | coveralls",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/olliru/tuni-swtest-testgroup.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/olliru/tuni-swtest-testgroup/issues"
  },
  "homepage": "https://github.com/olliru/tuni-swtest-testgroup#readme",
  "devDependencies": {
    "@babel/cli": "^7.12.8",
    "@babel/preset-env": "^7.12.7",
    "@babel/register": "^7.12.1",
    "babel-plugin-istanbul": "^6.0.0",
    "chai": "^4.2.0",
    "coveralls": "^3.1.0",
    "cross-env": "^7.0.3",
    "istanbul": "1.0.0-alpha.2",
    "mocha": "^8.2.1",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^15.1.0"
  },
  "nyc": {
    "require": [
      "@babel/register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false,
    "exclude": "src/.internal/*.js" 
  }
}
